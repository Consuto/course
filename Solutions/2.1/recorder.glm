module residential;
module tape;

clock {
	timezone PST+8PDT;
	timestamp '2001-01-01 0:00:00';
}

object house:1 {
}

object recorder {
	parent house:1;
	property air_temperature;
	file "tair.csv";
	interval 3600;
	limit 8760;
}
		
	
object waterheater:2 {
	parent house:1;
}
object recorder {
	parent waterheater:2;
	property constant_power;
	file "power.csv";
	interval -1;
	limit 100;
}


