module residential;
module tape;

clock {
	timezone PST+8PDT;
	starttime '2001-01-01 0:00:00 PST';
}

object house {
	object recorder {
		property air_temperature;
		file "tair.csv";
		interval 3600;
		limit 8760;
	};
	object waterheater {
		object recorder {
			property power;
			file "power.csv";
			interval -1;
			limit 100;
		};
	};
}