// Exercise 3.1

#set suppress_repeat_messages=0

module climate;
module tape;

clock {
	timezone "PST+8PDT";
	starttime '2001-01-01 0:00:00';
	stoptime '2001-01-02 0:00:00';
}

object climate {
	name "Yakima WA";
	tmyfile "WA-Yakima.tmy2";
	interpolate QUADRATIC; // LINEAR
}

object recorder {
	parent "Yakima WA";
	property temperature,humidity;
	interval 600;
	limit 144;
	file "exercise_3_1_2.csv";
}